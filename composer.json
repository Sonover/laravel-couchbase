{
  "name": "sonover/couchbase",
  "description": "A Couchbase based Eloquent model and Query builder for Laravel",
  "keywords": ["laravel","eloquent","couchbase","database","model"],
  "homepage": "https://github.com/sonover/laravel-couchbase",
  "authors": [
  ],
  "license" : "MIT",
  "require": {
    "illuminate/support": "^5.4",
    "illuminate/container": "^5.4",
    "illuminate/database": "^5.4",
    "illuminate/events": "^5.4",
    "ext-couchbase": ">=2.2.0",
    "ramsey/uuid": "^3.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0|^5.0",
    "orchestra/testbench": "^3.4",
    "orchestra/database": "~3.1",
    "mockery/mockery": "^0.9"
  },
  "autoload": {
    "psr-4": {
      "Sonover\\Couchbase\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sonover\\Couchbase\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
